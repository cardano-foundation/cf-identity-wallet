{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "node",
        "outDir": "../build/",
        "sourceMap": false,
        "target": "es2019",
        "module": "commonjs",
        "removeComments": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "types": [
            "node",
            "webdriverio/async",
            "@wdio/mocha-framework",
            "expect-webdriverio"
        ]
    }
}
